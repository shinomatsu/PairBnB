        <%= text_field_tag :search, class: "form-control", placeholder:"aaa" %>
        <span class="input-group-btn">
          <%= submit_tag "Submit", class: "btn btn-secondary", type: "button" %>
        </span>
        <% end %>
      </div>
    </div>
  </div>
</div>



