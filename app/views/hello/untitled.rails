<div class="container">
<%= form_tag listings_path, :method => 'get' do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :listings => nil %>
        
   



<% end %>

    
</div>


<div class="container">
  <div class="row">
    <div class="col-sm-6 hidden-sm-down">
      <div class="input-group">
